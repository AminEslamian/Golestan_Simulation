@model Teachs;

@{
    ViewData["Title"] = "Unassign Instructor";
}
<h2>
    Are you sure to unassign this account from the section?
</h2>

<div>
    <form asp-action="UnassignInstructor">
        <input asp-for="InstructorId" type="hidden" value=@Model.InstructorId />
        <input asp-for="SectionId" type="hidden" value=@Model.SectionId />
        <input type="submit" value="Unassign" class="btn btn-danger" />
    </form>
</div>